{
  "name": "prom-cms",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "generate:dev": "npm run generate:dev -w @prom-cms/cli",
    "build:packages": "npx turbo run build --filter=./packages/*",
    "build:admin": "npx turbo run build --filter=@prom-cms/admin",
    "build": "npm run build:packages",
    "dev": "npx turbo run dev --filter=@prom-cms/admin...",
    "prettier": "npx turbo run prettier --filter=./packages/*",
    "test": "npx turbo run test --filter=./packages/*",
    "release": "npm run build:packages && changeset publish",
    "version-packages": "changeset version && npx prettier"
  },
  "license": "MIT",
  "workspaces": [
    "./packages/*",
    "./apps/*"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/"
  },
