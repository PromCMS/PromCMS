{
  "name": "prom-cms",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "build:library": "yarn preconstruct build",
    "postinstall": "yarn preconstruct dev",
    "release": "yarn build:library && changeset publish",
    "clean": "del **/dist/** && del **/.next/**",
    "dev": "turbo run dev --parallel",
    "dev:api": "turbo run dev --scope=\"@prom-cms/dev-api\" --parallel",
    "generate:dev": "yarn workspace @prom-cms/cli run generate:dev",
    "generate:cms": "yarn workspace @prom-cms/cli run generate:cms",
    "regenerate:dev": "yarn workspace @prom-cms/cli run regenerate:dev",
    "regenerate:cms": "yarn workspace @prom-cms/cli run regenerate:cms",
    "prettier": "prettier --write \"packages/*/{*.{ts,js,scss,css},!{_next, dist}/**/*}\"",
    "version-packages": "changeset version && yarn prettier"
  },
  "license": "MIT",
  "workspaces": [
    "./packages/*"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/"
  },
  "contributors": [
    "Ond≈ôej Langr <hi@ondrejlangr.cz>"
  ],
  "devDependencies": {
    "@preconstruct/cli": "2.2.0",
    "@prettier/plugin-php": "0.17.6",
    "@babel/preset-react": "^7.18.6",
    "@swc/core": "1.2.120",
    "@types/node": "16.11.10",
    "del-cli": "4.0.1",
    "prettier": "2.6.2",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@changesets/cli": "^2.22.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "rimraf": "3.0.2"
  },
  "preconstruct": {
    "packages": [
      "packages/*",
      "!packages/admin",
      "!packages/config",
      "!packages/dev-api"
    ]
  }
}
