{
  "name": "prom-cms",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "generate:dev": "yarn workspace @prom-cms/cli run generate:dev",
    "build:packages": "turbo run build --filter=./packages/*",
    "build": "npm run build:packages",
    "dev:admin": "yarn workspace @prom-cms/admin run dev",
    "dev:api": "yarn workspace @prom-cms/dev-api run dev",
    "dev": "yarn concurrently \"npm run dev:api\" \"npm run dev:admin\"",
    "prettier": "yarn turbo run prettier --filter=./packages/*",
    "test": "yarn turbo run test --filter=./packages/*",
    "release": "yarn build:packages && changeset publish",
    "version-packages": "changeset version && yarn prettier"
  },
  "license": "MIT",
  "workspaces": [
    "./packages/*",
    "./apps/*"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/"
  },
  "contributors": [
    "OndÅ™ej Langr <hi@ondrejlangr.cz>"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@prettier/plugin-php": "0.17.6",
    "@swc/core": "^1.3.21",
    "@vitest/coverage-c8": "^0.25.3",
    "caniuse-lite": "^1.0.30001387",
    "concurrently": "^7.2.2",
    "del-cli": "4.0.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "tsup": "^6.5.0",
    "turbo": "^1.6.3"
  },
  "overrides": {
    "react": "^18.2.0",
    "@types/react": "^18.0.24"
  }
}
