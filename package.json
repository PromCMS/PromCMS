{
  "name": "prom-cms",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "build:library": "turbo run build --filter=./packages/*",
    "release": "yarn build:library && changeset publish",
    "clean": "del **/dist/** && del **/.next/**",
    "dev": "turbo run dev --no-cache --parallel --continue --filter=./apps/*",
    "generate:dev": "yarn workspace @prom-cms/cli run generate:dev",
    "regenerate:dev": "yarn workspace @prom-cms/cli run regenerate:dev",
    "prettier": "prettier --config ./packages/config/prettier-config.js --write \"**/*.{ts,tsx}\"",
    "version-packages": "changeset version && yarn prettier"
  },
  "license": "MIT",
  "workspaces": [
    "./packages/*",
    "./apps/*"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/"
  },
  "contributors": [
    "Ond≈ôej Langr <hi@ondrejlangr.cz>"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@prettier/plugin-php": "0.17.6",
    "@swc/core": "1.2.120",
    "@types/node": "16.11.10",
    "del-cli": "4.0.1",
    "prettier": "2.6.2",
    "tsup": "^6.1.3",
    "turbo": "^1.3.1",
    "typescript": "^4.8.0-dev.20220719"
  },
  "dependencies": {
    "rimraf": "3.0.2"
  }
}
