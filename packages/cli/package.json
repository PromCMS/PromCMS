  "main": "dist/prom-cms-cli.cjs.js",
  "module": "dist/prom-cms-cli.esm.js",
  "typings": "dist/prom-cms-cli.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist/**"
  ],
  "bin": {
    "prom-cli": "./bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PromCMS/PromCMS.git"
  },
  "scripts": {
    "starter-commands": "node --loader=ts-node/esm --loader ./loader.mjs --experimental-specifier-resolution=node src/index.ts",
    "generate:dev": "npm run starter-commands -- generate:develop && npm run seed-database",
    "generate:cms": "npm run starter-commands -- generate:cms",
    "regenerate:dev": "npm run starter-commands -- generate:develop --regenerate",
    "regenerate:cms": "npm run starter-commands -- generate:cms --regenerate",
    "seed-database": "npm run starter-commands -- seed-database",
    "build": "tsc --build"
  },
  "bugs": {
    "url": "https://github.com/PromCMS/PromCMS/issues"
  },
