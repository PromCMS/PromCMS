{
  "name": "@prom-cms/cli",
  "version": "0.5.0",
  "description": "PromCMS cli package",
  "author": "Ond≈ôej Langr <hi@ondrejlangr.cz>",
  "homepage": "https://github.com/PromCMS/PromCMS/blob/main/packages/clie/README.md",
  "license": "MIT",
  "type": "module",
  "main": "dist/prom-cms-cli.cjs.js",
  "module": "dist/prom-cms-cli.esm.js",
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "templates",
    "scripts"
  ],
  "bin": {
    "prom-cli": "./bin/cli.cjs"
  },
  "scripts": {
    "starter-commands": "node --loader=ts-node/esm --loader ./loader.mjs --experimental-specifier-resolution=node src/index.ts",
    "generate:dev": "npm run starter-commands -- generate:develop && npm run sync-database ../../apps/dev-api/.temp && npm run seed-database ../../apps/dev-api/.temp",
    "generate:cms": "npm run starter-commands -- generate-cms",
    "regenerate:dev": "npm run starter-commands -- generate:develop --regenerate",
    "regenerate:cms": "npm run starter-commands -- generate-cms --regenerate",
    "seed-database": "npm run starter-commands -- seed-database",
    "sync-database": "npm run starter-commands -- sync-database"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PromCMS/PromCMS.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/PromCMS/PromCMS/issues"
  },
  "dependencies": {
    "ink": "3.2.0",
    "@prom-cms/shared": "*",
    "ejs": "^3.1.8",
    "enquirer": "2.3.6",
    "@faker-js/faker": "6.0.0",
    "@boost/cli": "3.0.3",
    "chalk-animation": "1.6.0",
    "chalk": "5.0.0",
    "execa": "6.0.0",
    "react": "^18.2.0",
    "fs-extra": "10.0.0",
    "prettier": "2.5.1",
    "dotenv-cli": "^5.1.0",
    "dayjs": "1.10.7"
  },
  "devDependencies": {
    "@types/prettier": "2.4.2",
    "@prom-cms/config": "*",
    "@types/ejs": "^3.1.1",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "@types/fs-extra": "9.0.13"
  }
}
